{
  "name": "ts-cli-app",
  "version": "1.0.0",
  "description": "A TypeScript CLI application",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "ts-cli": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node --esm --experimental-specifier-resolution=node src/index.tsx",
