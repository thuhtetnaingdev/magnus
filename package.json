    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "cli",
    "typescript",
    "nodejs"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.6.1",
    "ink": "^6.4.0",
    "react": "^19.2.0",
    "winston": "^3.18.3",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^19.2.2",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  }
}
